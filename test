public onTabChange(tabValue: string): void {
  if (tabValue === 'completed') {
    this.router.navigate(['/ticket-list/terminated-list']);
  } else {
    this.router.navigate(['/ticket-list'], { queryParams: { status: tabValue } });
  }
}

public onTabChange(tabValue: string): void {
  if (tabValue === 'current') {
    this.router.navigate(['/ticket-list'], { queryParams: { status: tabValue } });
  } else {
    this.router.navigate(['/ticket-list/terminated-list']);
  }
}
