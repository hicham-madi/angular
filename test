onTabClicked(tabValue: string): void {
  const url = this.router.url;
  const isTeam = url.includes('team-list');

  if (tabValue === 'terminated-list') {
    this.router.navigate(['/ticket-list/terminated-list']);
  } else {
    const base = isTeam ? ['/ticket-list', 'team-list'] : ['/ticket-list'];
    this.router.navigate([...base, 'status', tabValue]);
  }
}

{ label: 'TerminÃ©es', value: 'terminated-list' }
