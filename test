if (cardParam && FilterCardType[cardParam as keyof typeof FilterCardType]) {
  this.activeCard = FilterCardType[cardParam as keyof typeof FilterCardType];
} else {
  this.activeCard = FilterCardType.NONE;
}

const cardParam = params.get('card') as keyof typeof FilterCardType;

this.activeCard = FilterCardType[cardParam] ?? FilterCardType.NONE;
