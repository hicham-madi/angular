ngOnInit(): void {
  this.router.events
    .pipe(filter((e): e is NavigationEnd => e instanceof NavigationEnd))
    .subscribe(() => {
      this.currentTab = this.router.url.includes('terminated-list') ? 'completed' : 'ongoing';
    });
}

