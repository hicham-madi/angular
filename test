public static Predicate<Object, Ticket> requestsExpertServiceEds(String edsId) {
    EntryObject e = Predicates.newPredicateBuilder().getEntryObject();

    return Predicates.and(
        e.get("customer.eds").equal(true),
        e.get("customer.edsId").equal(edsId),
        e.get("claimType.code").equal("DS")
    );
}

case REQUESTS_EXPERT_SERVICE_EDS -> TicketCriteria.requestsExpertServiceEds(edsId);
