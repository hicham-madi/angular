this.timelineRep = this.timeLine.find((e) => e.shortLabel === 'REP') 
  ?? this.timeLine.find((e) => e.shortLabel === 'end');

this.countDay = this.getCountDay();

this.bufferValue = this.timelineRep?.dayInBetweenDates != null
  ? Math.abs(this.timelineRep.dayInBetweenDates)
  : this.treatmentDelay ?? 0;
