ngOnInit(): void {
  this.router.events.subscribe(event => {
    if (event instanceof NavigationEnd) {
      const match = Array.from(this.tabsUrls).find(([tab, url]) =>
        event.urlAfterRedirects.includes(url)
      );

      if (match) {
        this.activeTab = match[0];
      }
    }
  });
}
