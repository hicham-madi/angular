public static Predicate<Ticket> treatmentIsLateDsPredicate(String userId, String edsId, List<String> edsChilds, boolean isPrivate) {
    return ticket -> {
        if (!ticket.getClaimTypeCode().equals(ClaimTypeCode.DS)) return false;
        if (!ticket.getStatus().isIn(StatusTicket.PROCESSING, StatusTicket.PENDING, ...)) return false;
        if (isPrivate && !ticket.isPrivate()) return false;
        // Vérifie edsId / edsChilds si nécessaire
        return true;
    };
}
CARD_TYPE_PREDICATES.put(
    FilterCardType.TREATMENT_IS_LATE_DS,
    (ticket, userId, edsId, edsChilds, isPrivate) ->
        TicketCriteria.treatmentIsLateDsPredicate(userId, edsId, edsChilds, isPrivate).test(ticket)
);
