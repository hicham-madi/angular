case REQUESTS:
    return !StringUtils.isEmpty(ticket.getClaimType().getCode())
        && ("D".equals(ticket.getClaimType().getCode()) || "DC".equals(ticket.getClaimType().getCode()));

case CLAIMS:
    return !StringUtils.isEmpty(ticket.getClaimType().getCode())
        && "RC".equals(ticket.getClaimType().getCode());
