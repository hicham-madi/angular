public List<Ticket> getTicketsForClaimTypeD_DC(String envId, String customerId) {
    Predicate<Object, Ticket> predicate = Predicates.and(
        Predicates.in("claimType.code", "D", "DC"),
        Predicates.in("status", "NEW", "PROCESSING", "PENDING", "APPROVAL_PENDING", "APPROVAL_REFUSED"),
        Predicates.equal("customer._id", customerId)
    );

    IMap<Object, Ticket> map = client.getMap("tickets_" + envId);
    Collection<Ticket> results = map.values(predicate);
    return new ArrayList<>(results);
}
