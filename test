localStorage.setItem('ticketFilters', JSON.stringify(this.listService.getCurrentParams()));

const savedFilters = localStorage.getItem('ticketFilters');
if (savedFilters) {
  const filters = JSON.parse(savedFilters);
  this.listService.setFilters(filters); // méthode à créer si elle n’existe pas
}

public setFilters(filters: any): void {
  this.currentParams = filters;
  // ou directement appliquer les filtres si nécessaire
}
