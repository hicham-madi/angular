protected onSearchTypeFilterChanged(selectedSearchType: string): void {
  const searchTypeMap: Record<string, string> = {
    'Nom de Client': 'PART_NAME',
    'Numéro de compte': 'FORM_VALUES',
    'Numéro de ticket': 'TICKET_ID'
  };

  this.selectedSearchType = searchTypeMap[selectedSearchType] ?? '';
  this.applyFilters();
}
