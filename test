protected getDsRemainingDays(row: Ticket): number | null {
  const endItem = row?.ds?.timeline?.find(t => t.shortLabel === 'end');
  return endItem?.remainingDays ?? null;
}

<metabot-bar-timeline
  [treatmentDate]="getDsTreatmentDate(row)"
  [timeline]="[getDsEndTimeline(row)]"
  [date]="getDsRemainingDays(row)">
</metabot-bar-timeline>
