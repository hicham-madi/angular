Predicate<Ticket> predicateCustomerId =
    ticket -> customerId.equals(ticket.getCustomerId());

Predicate<Ticket> predicateIsDsAffectedToEdsId =
    ticket -> filters.getEds().contains(ticket.getCustomer().getEdsId());

Predicate<Ticket> globalDsPredicate = predicateClaimTypeDS
    .and(predicateStatus)                     // ton prédicat de statut global
    .and(predicatePrivate)                    // prédicat de confidentialité
    .and(predicateCustomerId.or(predicateIsDsAffectedToEdsId))
    .and(predicateStatusValidationExpert)     // status = VALIDATION_EXPERT
    .and(predicateReadCustomerTrue);          // readCustomer = true
