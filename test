switch (this.currentContext) {
  case TicketContext.SOLICITATIONS: {
    if (!this.setCountSolicitations) {
      this.tableConfigFactory.ticketsCountSolicitations = totalElements;
      this.setCountSolicitations = true;
    }
    this.ticketsCountSolicitations = this.tableConfigFactory.ticketsCountSolicitations;
    break;
  }

  case TicketContext.TEAM: {
    if (!this.setCountTeam) {
      this.tableConfigFactory.ticketsCountTeam = totalElements;
      this.setCountTeam = true;
    }
    this.ticketsCountTeam = this.tableConfigFactory.ticketsCountTeam;
    break;
  }

  case TicketContext.TERMINATED: {
    if (!this.setCountTerminated) {
      this.tableConfigFactory.ticketsCountTerminated = totalElements;
      this.setCountTerminated = true;
    }
    this.ticketsCountTerminated = this.tableConfigFactory.ticketsCountTerminated;
    break;
  }

  case TicketContext.TERMINATED_TEAM: {
    if (!this.setCountTerminatedTeam) {
      this.tableConfigFactory.ticketsCountTerminatedTeam = totalElements;
      this.setCountTerminatedTeam = true;
    }
    this.ticketsCountTerminatedTeam = this.tableConfigFactory.ticketsCountTerminatedTeam;
    break;
  }
}
